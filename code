int print2largest(vector<int> &arr) {
        int max=arr[0];
        for(int i=1;i<arr.size();i++){
            if(arr[i]>max){
                max=arr[i];
            }
        }
        
        int secmax=-1;
        for(int i=0;i<arr.size();i++){
            if(arr[i]!=max && arr[i]>secmax){
                secmax=arr[i];
            }
        }
        return secmax;
        
        
    }
